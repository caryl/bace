<%= form.error_messages %>

<dl>
  <dt><%= form.label :klass_id %></dt>
  <dd><%= form.select :klass_id, Klass.all.map{|k|[k.name, k.id]} %></dd>
  <dt><%= form.label :key %></dt>
  <dd><%= form.text_field :key %></dd>
  <dt><%= form.label :name %></dt>
  <dd><%= form.text_field :name %></dd>
  <dt><%= form.label :assoc_klass_id %></dt>
  <dd><%= form.select :assoc_klass_id, Klass.all.map{|k|[k.name, k.id]}, :include_blank => true %></dd>
  <dt><%= form.label :include %></dt>
  <dd><%= form.text_field :include %></dd>
  <dt><%= form.label :joins %></dt>
  <dd><%= form.text_field :joins %></dd>
  <dt><%= form.label :editor %></dt>
  <dd><%= form.text_field :editor %></dd>
  <dt><%= form.label :kind_id %></dt>
  <dd><%= form.select :kind_id, Meta::KINDS %></dd>
</dl>
